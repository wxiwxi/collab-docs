{
    "name": "@wangx-doc/react",
    "private": false,
    "sideEffects": [
        "*.css"
    ],
    "license": "MIT",
    "version": "1.19.0",
    "keywords": [
        "wangx",
        "editor",
        "typescript",
        "prosemirror",
        "wysiwyg",
        "rich-text-editor",
        "feishu",
        "notion",
        "yjs",
        "block-based",
        "tiptap"
    ],
    "description": "基于 Prosemirror 和 Tiptap 实现的类 Notion、飞书文档风格可扩展文本编辑器。",
    "type": "module",
    "source": "src/index.ts",
    "main": "build/cjs/index.js",
    "module": "build/esm/index.js",
    "exports": {
        ".": {
            "import": "./build/esm/index.js"
        },
        "./style.css": {
            "import": "./build/esm/index.css"
        }
    },
    "scripts": {
        "build:watch": "tsup --watch",
        "build": "tsup",
        "clean": "rimraf build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@wangx-doc/core": "workspace:*",
        "csstype": "^3.1.3",
        "@floating-ui/react": "^0.26.4",
        "@tiptap/core": "^2.7.1",
        "@tiptap/react": "^2.7.1",
        "lodash.merge": "^4.6.2",
        "react-icons": "^5.2.1"
    },
    "devDependencies": {
        "@types/lodash.foreach": "^4.5.9",
        "@types/lodash.groupby": "^4.6.9",
        "@types/lodash.merge": "^4.6.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9"
    },
    "peerDependencies": {
        "react": "^18.0 || ^19.0 || >= 19.0.0-rc",
        "react-dom": "^18.0 || ^19.0 || >= 19.0.0-rc"
    }
}
