        "remark-rehype": "^10.1.0",
        "remark-stringify": "^10.0.2",
        "shiki": "^1.22.0",
        "unified": "^10.1.2",
        "uuid": "^8.3.2",
        "y-prosemirror": "1.2.12",
        "y-protocols": "^1.0.6",
        "yjs": "^13.6.15"
    },
