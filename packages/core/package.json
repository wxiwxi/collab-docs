{
    "name": "@wangx-doc/core",
    "private": false,
    "sideEffects": [
        "*.css"
    ],
    "license": "MIT",
    "version": "1.19.0",
    "keywords": [
        "wangx",
        "editor",
        "typescript",
        "prosemirror",
        "wysiwyg",
        "rich-text-editor",
        "feishu",
